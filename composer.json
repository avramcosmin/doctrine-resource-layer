{
    "name": "mindlahus/doctrine-resource-layer",
    "description": "An abstract class that helps manage relationships with Doctrine and Symfony.",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "avramcosmin/forceutf8",
                "version": "2.0.0",
                "source": {
                    "url": "https://github.com/avramcosmin/forceutf8",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],
    "require": {
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "symfony/symfony": "^3.2",
        "php": "^7.1",
        "symfony/monolog-bundle": "^3.1",
        "erusev/parsedown-extra": "^0.7.1",
        "friendsofsymfony/rest-bundle": "^2.2",
        "jms/serializer-bundle": "^2.0",
        "nelmio/cors-bundle": "^1.5",
        "gedmo/doctrine-extensions": "^2.4",
        "stof/doctrine-extensions-bundle": "^1.2",
        "avramcosmin/forceutf8": "^2.0",
        "phpseclib/phpseclib": "^2.0",
        "mindlahus/symfony-assets": "^1.1"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^3.2",
        "mockery/mockery": "^0.9.9"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Avram Cosmin",
            "email": "avram.cosmin@sprindo.com",
			"homepage": "http://www.sprindo.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Mindlahus\\DoctrineResourceLayer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Mindlahus\\DoctrineResourceLayer\\": "tests/"
        }
    }
}
